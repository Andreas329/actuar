### C code
useDynLib(actuar, .registration = TRUE)

### Functions
export(## Credibility theory
       bstraub, rearrangepf, simpf,
       ## Risk theory
       panjer,
       ## One parameter distributions
       dinvexp, pinvexp, qinvexp, rinvexp,
       ## Two parameter distributions
       dllogis, pllogis, qllogis, rllogis,
       dparalogis, pparalogis, qparalogis, rparalogis,
       dinvgamma, pinvgamma, qinvgamma, rinvgamma,
       dinvparalogis, pinvparalogis, qinvparalogis, rinvparalogis,
       dpareto, ppareto, qpareto, rpareto,
       dpareto2, ppareto2, qpareto2, rpareto2, # aliases
       dinvpareto, pinvpareto, qinvpareto, rinvpareto,
       dpareto1, ppareto1, qpareto1, rpareto1,
       dinvweibull, pinvweibull, qinvweibull, rinvweibull,
       dlgompertz, plgompertz, qlgompertz, rlgompertz,          # aliases
       ## Three parameter distributions
       dburr, pburr, qburr, rburr,
       dinvburr, pinvburr, qinvburr, rinvburr,
       dgenpareto, pgenpareto, qgenpareto, rgenpareto,
       dtrgamma, ptrgamma, qtrgamma, rtrgamma,
       dinvtrgamma, pinvtrgamma, qinvtrgamma, rinvtrgamma,
       ## Four parameter distributions
       dtrbeta, ptrbeta, qtrbeta, rtrbeta,
       dpearson4, ppearson4, qpearson4, rpearson4 #aliases
       )

### Methods
S3method(print, bstraub)
S3method(summary, bstraub)
S3method(print, summary.bstraub)
S3method(print, simpf)
S3method(summary, simpf)
S3method(print, summary.simpf)

